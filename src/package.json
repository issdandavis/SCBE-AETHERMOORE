{
  "name": "@scbe/production-pack",
  "version": "3.0.0",
  "description": "SCBE-AETHERMOORE: 14-layer AI governance framework with post-quantum cryptography, Sacred Tongues, and hyperbolic geometry",
  "type": "commonjs",
  "main": "crypto/envelope.js",
  "types": "crypto/index.ts",
  "exports": {
    ".": "./crypto/index.ts",
    "./crypto": "./crypto/index.ts",
    "./crypto/*": "./crypto/*.ts"
  },
  "scripts": {
    "build": "tsc",
    "test": "node --experimental-vm-modules node_modules/vitest/vitest.mjs --run",
    "test:watch": "node --experimental-vm-modules node_modules/vitest/vitest.mjs",
    "typecheck": "tsc --noEmit",
    "example": "node examples/usage.js",
    "test:py": "python -m pytest ../tests/ -x -q",
    "start:api": "python -m uvicorn api.saas_api:app --host 0.0.0.0 --port 8000"
  },
  "keywords": [
    "ai-governance",
    "post-quantum-cryptography",
    "aes-gcm",
    "sacred-tongues",
    "hyperbolic-geometry",
    "bft-consensus",
    "ai-safety",
    "browser-automation",
    "scbe",
    "aethermoore"
  ],
  "author": "issdandavis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/issdandavis/SCBE-AETHERMOORE.git"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.17"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
